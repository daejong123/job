<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.job.mapper.JobMapper">
    <!-- 自定义返回结果集 -->
    <resultMap id="jobMap" type="JobEntity">
        <id property="jobId" column="id"></id>
        <result property="jobName" column="name"></result>
        <result property="pubTime" column="time"></result>
        <result property="urlInfo" column="link"></result>
    </resultMap>
</mapper>